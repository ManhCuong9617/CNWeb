@model Model.Entities.GroupPr
@{
    ViewBag.Title = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

@section jsFooter{
    <script src="~/assets/admin/js/updateAdmin.js"></script>
}
<div id="page-wrapper">
    <div class="col-md-12">
        <div class="form-horizontal">
            <div class="form-group">
                <label class="control-label col-sm-3" for="ma">Mã nhóm sản phẩm: </label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" id="ma" value="@Model.maNhom" readonly>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-3" for="ten">Tên nhóm sản phẩm: </label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" id="ten" value="@Model.tenNhom">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-3" for="tittle">Meta Tittle: </label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" id="tittle" value="@Model.meta_tittle">
                </div>
            </div>
            <button type="button" class="btn btn-default" id="btnUpdateGroupPr">Lưu thay đổi</button>
        </div>

    </div>
</div>

